    </div>
    <div id="mapBtn"><img src="./res/temp_0003.gif"> </div>
    <div class="snsAndCopyright">
      
      <div class="c"></div>
      <div id="copyright">版权所有 © 20014-2015  YrealSoft.Com </div>
    </div>
    <div id="qrCode"> <img src="./res/temp_0004.gif" width="100" height="100"> </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#footer .corner').click(function () {
              $('#footer').hide(0);
          });
          $('a,input[type="button"],input[type="submit"]').bind('focus', function () {
              if (this.blur) {
                  this.blur();
              };
          });
          $('#qrCode img').hover(function () {
              $(this).stop().animate({ width: '280px', height: '280px', left: '-120px', top: '-131px' }, 'fast', function () {
                  $('#footer .corner').css('display', 'none'); // 这个无辜，就是影响了些二维码的形象
              });
          }, function () {
              $('#footer .corner').css('display', 'block');
              $(this).animate({ width: '100px', height: '100px', left: '15px', top: '15px' }, 'fast');
          });
          // 底部的地图
          $('#mapBtn img').click(function () {
              $('#map').fadeIn();
          });
          $('#map a').click(function () {
              $('#map').fadeOut();
          });
      });
	</script>
  <link href="./res/backtop.css" type="text/css" rel="stylesheet" charset="utf-8">
  <p id="backTop" style="display: none;"> <a href="###top" title="点击返回顶部"><span></span></a> </p>
  <script>
      $(function () {
          $("#backTop").hide();
          $(function () {
              $(window).scroll(function () {
                  if ($(this).scrollTop() > 100) {
                      $("#backTop").fadeIn();
                  } else {
                      $("#backTop").fadeOut();
                  }
              });
              $("#backTop a").click(function () {
                  $("body,html").animate({
                      scrollTop: 0
                  }, 200);
                  return false;
              });
          });
      });
		</script> 
